{
  "name": "chat-app-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:backend": "node backend/index.js",
    "dev:frontend": "cd frontend && next dev -p 3000",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "next build ./frontend",
    "start:frontend": "next start ./frontend"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "next": "^14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
